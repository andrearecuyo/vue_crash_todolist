AAAA,UAGA,SAHA,GAKA,OALA,CAGA,SAHA;AAOA,MAAA,KAAA,CAAA,IAAA,CAAA,4CAAA,EAAA;AACA,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,SAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,GAAA;AAAA,eAAA,MAAA,CAAA,KAAA,gCAAA,MAAA,CAAA,KAAA,IAAA,GAAA,CAAA,IAAA,EAAA;AAAA,OAJA,EAKA,KALA,CAKA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OALA;AAMA;AAnBA,GA5BA;AAiDA,EAAA,OAjDA,qBAiDA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,sDAAA,EACA,IADA,CACA,UAAA,GAAA;AAAA,aAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA;AAAA,KADA,EAEA,KAFA,CAEA,UAAA,GAAA;AAAA,aAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,KAFA;AAGA;AArDA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <AddTodos v-on:add-todo=\"addTodo\"/>\n    <Todos v-bind:todos=\"todos\" v-on:del-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport Todos from '../components/Todos';\nimport AddTodos from '../components/AddTodo';\nimport axios from 'axios';\n\nexport default {\n  name: 'Home',\n  components: {\n    Todos,\n    AddTodos\n  },\n  data() {\n    return {\n      todos: [\n        {\n          id: 1,\n          title: \"Todo One\",\n          completed:false\n        },\n        {\n          id: 2,\n          title: \"Todo Two\",\n          completed:true\n        },\n        {\n          id: 3,\n          title: \"Todo Three\",\n          completed: false\n        }\n      ]\n\n      }\n    },\n    methods: {\n      deleteTodo(id) {\n        axios.delete(`https://jsonplaceholder.typicode.com/todos/${id}`)\n          .then(res => this.todos = this.todos.filter(todo => todo.id !== id, res.data))\n          .catch(err => console.log(err));\n      },\n      addTodo(newTodo) {\n        const {\n          title, \n          completed\n        }\n        = newTodo;\n\n        axios.post('https://jsonplaceholder.typicode.com/todos', {\n          title, \n          completed\n        })\n        .then(res => this.todos = [...this.todos, res.data])\n        .catch(err => console.log(err))\n      }\n    },\n    created() {\n      axios.get('https://jsonplaceholder.typicode.com/todos?_limit=10')\n        .then(res => this.todos = res.data)\n        .catch(err => console.log(err))\n    }\n  }\n</script>\n\n<style>\n  * {\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n  }\n\n  #app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    color: #2c3e50;\n  }\n  \n  .btn {\n    display: inline-block;\n    border: none;\n    background-color: #555;\n    color: #fff;\n    padding: 7px 20px;\n    cursor: pointer;\n  }\n\n  .btn:hover {\n    background-color:  #666;\n  }\n</style>\n"],"sourceRoot":"src/views"}]}